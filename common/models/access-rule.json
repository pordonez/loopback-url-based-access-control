{
  "name": "AccessRule",
  "base": "PersistedModel",
  "idInjection": true,
  "properties": {
    "baseUrl": {
      "type": "string"
    },
    "roleType": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "users": {
      "type": "hasMany",
      "model": "user",
      "foreignKey": "userId"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "principalType": "ROLE",
      "principalId": "accessManager",
      "permission": "ALLOW"
    }
  ],
  "methods": []
}
